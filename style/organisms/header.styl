// Variable
.tlt-header
	font-weight 600
	text-transform uppercase
	color $google-green

.header
	text-align center
	>.header-title
		@extend .tlt-header
		font-size 2.2rem
	>.header-text
		font-size .9rem
		font-weight 100
		color $white
		.anchor
			font-style italic
			text-transform uppercase
	>.partners-title
		@extend .tlt-header
		font-size 1.8rem
